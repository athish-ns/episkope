# Firebase Configuration
# Copy this file to .env and fill in your actual Firebase project values
# NEVER commit the actual .env file with real API keys to version control

VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Alternative React App prefix (for Create React App projects)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Groq AI Service Configuration
VITE_GROQ_API_KEY=your_groq_api_key_here
REACT_APP_GROQ_API_KEY=your_groq_api_key_here

# Email Service Configuration
VITE_EMAIL_SERVICE_URL=http://localhost:3001
REACT_APP_EMAIL_SERVICE_URL=http://localhost:3001

# Application Configuration
VITE_APP_NAME=Rehabilitation Centre Management
VITE_APP_VERSION=1.0.0
REACT_APP_NAME=Rehabilitation Centre Management
REACT_APP_VERSION=1.0.0
